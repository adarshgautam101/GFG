
class GfG
{
	boolean isBinary(String str)
	{
	    int count=0;
	    int count1=0;
	  if(str.length()==1){
	      return true;
	  }
	  else
	  {
	      for(int i=0;i<str.length();i++){
	          if(str.charAt(i)=='1')
	          {
	             count1++; 
	          }
	          else if(str.charAt(i)=='0'){
	              count++;
	          }
	      }
	  }
 boolean k=(count1 > 0 && count >0 )? true:false;
 return k;
	}
}
